package com.StudentManagement.Service;

import java.sql.ResultSet;
import java.sql.SQLException;

import org.springframework.dao.DataAccessException;
import org.springframework.jdbc.core.ResultSetExtractor;
import org.springframework.jdbc.core.RowMapper;

import com.StudentManagement.Entities.StudentEntity;

public class StudentRowMapper implements  RowMapper<StudentEntity>{

	public StudentEntity mapRow(ResultSet rs, int rowNum) throws SQLException {
		StudentEntity bean=new StudentEntity();
		bean.setId(rs.getInt(1));
		bean.setName(rs.getString(2));
		bean.setMobile(rs.getLong(3));
		bean.setCountry(rs.getString(4));
		return bean;
	}

}
